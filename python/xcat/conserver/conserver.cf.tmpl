access * {
  trusted 127.0.0.1;
  trusted localhost;
}
default * {
  logfile /var/log/consoles/&;
  timestamp 1lab;
  rw *;
  master localhost;
}
config * {
  sslrequired yes;
  sslcacertificatefile /etc/xcat/cert/ca.pem;
  sslcredentials /etc/xcat/cert/server-cred.pem;
}
default cyclades { type host; portbase 8000; portinc 1; }
default mrv { type host; portbase 2000; portinc 100; }
default ACS { type host; portbase 7000; portinc 1; }
